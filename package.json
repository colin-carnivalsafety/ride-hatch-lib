{
  "name": "ride-hatch-lib",
  "version": "1.0.26",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build:cjs": "tsc --declaration --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --declaration --module ESNext --outDir dist/esm",
    "build": "tsc && yarn build:cjs && yarn build:esm",
    "deploy": "yarn version --patch && yarn build && git commit -am \"New release\" && git push --follow-tags"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
